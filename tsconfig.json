{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "custom-typings"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "shared-worker": [
        "dist/shared-worker"
      ],
      "shared-worker/*": [
        "dist/shared-worker/*"
      ],
      "worker-message": [
        "dist/worker-message"
      ],
      "worker-message/*": [
        "dist/worker-message/*"
      ]
    }
  }
}